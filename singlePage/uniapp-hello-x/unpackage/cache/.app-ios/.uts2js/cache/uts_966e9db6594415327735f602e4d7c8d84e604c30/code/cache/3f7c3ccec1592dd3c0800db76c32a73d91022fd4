{"code":"import { defineComponent } from \"vue\";\nimport refreshBox from '../../template/custom-refresher/refresh-box/refresh-box.uvue';\nexport default defineComponent({\n    components: { refreshBox },\n    data() {\n        return {\n            scrollData: [],\n            scrollTop: 0,\n            refresherTriggered: false,\n            pullingDistance: 0,\n            resetting: false\n        };\n    },\n    computed: {\n        state() {\n            if (this.resetting) {\n                return 3;\n            }\n            if (this.refresherTriggered) {\n                return 2;\n            }\n            if (this.pullingDistance > 45) {\n                return 1;\n            }\n            else {\n                return 0;\n            }\n        }\n    },\n    onLoad() {\n        let lists = [];\n        for (let i = 0; i < 30; i++) {\n            lists.push(\"item---\" + i);\n        }\n        this.scrollData = lists;\n    },\n    methods: {\n        onRefresherpulling(e) {\n            this.pullingDistance = e.detail.dy;\n        },\n        onRefresherrefresh() {\n            this.refresherTriggered = true;\n            setTimeout(() => {\n                this.refresherTriggered = false;\n                this.resetting = true;\n            }, 1500);\n        },\n        onRefreshrestore() {\n            this.pullingDistance = 0;\n            this.resetting = false;\n        },\n        //自动化测试使用\n        testBodyScrollBy(y) {\n            this.scrollTop = y;\n        }\n    }\n});\n//# sourceMappingURL=/Users/gaoruicheng/Documents/DcloudProject/uniapp-bugs/uniapp-bugs-main/singlePage/uniapp-hello-x/pages/component/nested-scroll-body/nested-scroll-body.uvue?vue&type=script&lang.uts.js.map","references":["/Users/gaoruicheng/Documents/DcloudProject/uniapp-bugs/uniapp-bugs-main/singlePage/uniapp-hello-x/pages/template/custom-refresher/refresh-box/refresh-box.uvue.ts"],"uniExtApis":[],"map":"{\"version\":3,\"file\":\"nested-scroll-body.uvue?vue&type