{
  "name": "uniapp-bugs-now-cli",
  "version": "1.0.0",
  "description": "uniapp 用于 bug 复现的模板项目",
  "main": "index.js",
  "scripts": {
    "all": "sh scripts/build-all.sh",
    "vue3": "cd vue3 && nr",
    "vue2": "cd vue2 && nr",
    "git-new-branch": "node ./scripts/git-new-branch.js",
    "git-new-branch all": "node ./scripts/git-new-branch.js all",
    "lint": "npm run lint:vue2 && npm run lint:vue3",
    "lint:fix": "npm run lint:vue2:fix && npm run lint:vue3:fix",
    "lint:vue2": "cd vue2 && npm run lint",
    "lint:vue2:fix": "cd vue2 && npm run lint:fix",
    "lint:vue3": "cd vue3 && npm run lint",
    "lint:vue3:fix": "cd vue3 && npm run lint:fix"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "child_process": "^1.0.2",
    "fs-extra": "^11.3.0"
  }
}
