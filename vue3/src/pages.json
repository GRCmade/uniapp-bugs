{
  "easycom": {
    "autoscan": true,
    "custom": {
      // uni-ui 规则如下配置
      "^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue"
    }
  },
  "pages": [
    //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
    {
      "path": "pages/index/index",
      "style": {
        "navigationBarTitleText": "uni-app"
      }
    },
    {
      "path": "pages/index/index-next",
      "style": {
        "navigationBarTitleText": "index-next"
      }
    },
    {
      "path": "pages/index/index-next-next",
      "style": {
        "navigationBarTitleText": "index-next-next"
      }
    },
    {
      "path": "pages/tabbar/tabbar-1/tabbar-1",
      "style": {
        "navigationBarTitleText": "tabbar-1"
      }
    },
    {
      "path": "pages/tabbar/tabbar-2/tabbar-2",
      "style": {
        "navigationBarTitleText": "tabbar-2"
      }
    }
  ],
  "subPackages": [
    {
      "root": "subPackage",
      "pages": [
        {
          "path": "pages/index",
          "style": {
            "navigationBarTitleText": "subpage"
          }
        },
        {
          "path": "visit/visit",
          "style": {
            "navigationBarTitleText": "visit"
          }
        }
      ]
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uni-app",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8"
  },
  "uniIdRouter": {},
  "tabBar": {
    "color": "#7A7E83",
    "selectedColor": "#3cc51f",
    "borderStyle": "black",
    "backgroundColor": "#ffffff",
    "list": [
      {
        "pagePath": "pages/index/index",
        "iconPath": "/static/logo.png",
        "selectedIconPath": "/static/logo-click.png",
        "text": "组件"
      },
      {
        "pagePath": "pages/tabbar/tabbar-1/tabbar-1",
        "iconPath": "/static/logo.png",
        "selectedIconPath": "/static/logo-click.png",
        "text": "接口"
      },
      {
        "pagePath": "pages/tabbar/tabbar-2/tabbar-2",
        "iconPath": "/static/logo.png",
        "selectedIconPath": "/static/logo-click.png",
        "text": "接口"
      }
    ]
  }
}
